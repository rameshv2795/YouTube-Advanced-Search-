        <img id="im1" src= "+vid_img" >
        <% if(locals.video_array !== undefined){ %>
          <% 
           // const dom = new locals.JSDOM(`<!DOCTYPE html><p>Hello world</p>`);
           // alert(dom.window.document.querySelector("p").textContent); // "Hello world"
            %>
            
            <% for(var i = 0; i < locals.video_array.length; i++){ %>
              <% var vid_img =  locals.video_array[i].pic %>
              
              <p><%= locals.video_array[i].name %><p>  
              <script> 
                var img_source = <%- JSON.stringify( locals.video_array[i].pic); %>;

                $('#mainid').prepend($('<img>',{src: img_source}));
                
                
                //$("#im1").attr("src",img_source);  
              </script> 
            <% } %>

          <% } %>
        <% if(locals.error !== null){ %>
            <p><%= locals.error %></p>
        <% } %>


      
      </div>      
    </div>

  </div>
